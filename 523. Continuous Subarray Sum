class Solution {
    public boolean checkSubarraySum(int[] nums, int k) {
        int n = nums.length;
        int sum = 0;
        for(int i = 0; i < n-1; i++){
            sum = nums[i];
            for(int j = i + 1; j < n; j++){
                sum += nums[j];
                if(sum % k == 0){
                    return true;
                }
            }
           
        }
        return false;
    }
}
