class Solution {
    public void moveZeroes(int[] nums) {
        int n = nums.length;
        int i = 0;
        int end = n-1;
        while(i <= end){
                if(nums[i] == 0){
                    for(int j = i; j < end; j++){
                        nums[j] = nums[j+1];
                    }
                    nums[end] = 0;
                        end--;
                }
                else{
                    i++;
                }
        }
        
    }
}
