class Solution {
    public int missingNumber(int[] nums) {
        // int size = nums.length;
        // for(int i = 0; i <= size; i++){
        //     boolean found = false;
        //     for(int num : nums){
        //         if(num == i){
        //             found = true;
        //             break;
        //         }
        //     }
        //     if(!found){
        //         return i;
        //     }
        // }

        // return -1;

        int n = nums.length;
        HashMap<Integer, Integer> map = new HashMap<>();
        for(int i = 0; i < n; i++){
            map.put(nums[i], i);
        }
        for(int j = 0; j <= n; j++){
           if(!map.containsKey(j)){
            return j;
           }
        }
        return -1;

    }
}
