class Solution {
    public int missingNumber(int[] nums) {
        int size = nums.length;
        for(int i = 0; i <= size; i++){
            boolean found = false;
            for(int num : nums){
                if(num == i){
                    found = true;
                    break;
                }
            }
            if(!found){
                return i;
            }
        }

        return -1;
        
    }
}
